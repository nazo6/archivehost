datasource db {
  provider = "sqlite"
  url      = "file:db"
}

generator client {
  provider      = "cargo prisma"
  output        = "../src/db/"
  module_path   = "crate::db"
  client_format = "folder"
}

model Archive {
  urlScheme String
  urlHost   String
  urlPath   String
  timestamp DateTime
  mime      String
  status    Int?
  savePath  String

  @@unique([urlScheme, urlHost, urlPath, timestamp])
}
